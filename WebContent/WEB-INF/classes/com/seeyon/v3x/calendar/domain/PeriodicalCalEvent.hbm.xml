<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.seeyon.v3x.calendar.domain.PeriodicalCalEvent" lazy="false" table="periodical_cal_event">
	<id
        name="id"
        type="long"
        column="id"
        length="20"
    >
		<generator class="assigned" />
	</id>
	<property
        name="alarmDate"
        type="long"
        column="alarm_date"
        length="20"
    />
    <property
        name="beforendAlarm"
        type="long"
        column="beforend_alarm_date"
        length="20"
    />
	<property
        name="alarmFlag"
        type="boolean"
        column="alarm_flag"
        not-null="true"
        length="11"
    />
	<property
        name="beginDate"
        type="java.util.Date"
        column="begin_date"
        length="19"
    />
	<property
        name="completeRate"
        type="float"
        column="complete_rate"
        not-null="true"
        length="12"
    />
	<property
        name="createDate"
        type="java.util.Date"
        column="create_date"
        length="19"
    />
	<property
        name="createUserId"
        type="long"
        column="create_user_id"
        length="20"
    />
	<property
        name="endDate"
        type="java.util.Date"
        column="end_date"
        length="19"
    />
	<property
        name="eventType"
        type="integer"
        column="event_type"
        length="11"
    />
	<property
        name="priorityType"
        type="integer"
        column="priority_type"
        length="11"
    />
	<property
        name="realEstimateTime"
        type="float"
        column="real_estimate_time"
        length="12"
    />
	<property
        name="shareTarget"
        type="string"
        column="share_target"
        length="65535"
    />
    <property
        name="receiveMemberId"
        type="string"
        column="receive_member_id"
        length="65535"
    />
    
    <property
        name="receiveMemberName"
        type="string"
        column="receive_member_name"
        length="65535"
    />
    <property
        name="tranMemberIds"
        type="string"
        column="tran_member_ids"
        length="65535"
    />
    
	<property
        name="shareType"
        type="integer"
        column="share_type"
        length="11"
    />
	<property
        name="signifyType"
        type="integer"
        column="signify_type"
        length="11"
    />
	<property
        name="states"
        type="integer"
        column="states"
        not-null="true"
        length="11"
    />
	<property
        name="subject"
        type="string"
        column="subject"
        not-null="true"
        length="500"
    />
	
	<property
        name="updateDate"
        type="java.util.Date"
        column="update_date"
        length="19"
    />
	<property
        name="workType"
        type="integer"
        column="work_type"
        length="11"
    />
<property
        name="eventflag"
        type="integer"
        column="event_flag"
        length="11"
    />
    <property
        name="attachmentsFlag"
        type="boolean"
        column="att_flag"
    />
    <property
			name="fromType"
			column="from_type"
			type="int"
			not-null="false"
			length="4"
		/>
		<property
			name="fromId"
			column="from_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
        name="calEventType"
        type="integer"
        column="cal_event_type"
        length="11"
   		/>
   		<property
        	name="periodicalStyle"
        	type="integer"
        	column="periodical_style"
        	length="11"
   		/>
	<!-- Associations -->

</class>
</hibernate-mapping>
