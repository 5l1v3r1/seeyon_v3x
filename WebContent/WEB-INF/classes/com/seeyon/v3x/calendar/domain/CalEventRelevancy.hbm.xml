<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.seeyon.v3x.calendar.domain.CalEventRelevancy" lazy="false" table="cal_event_relevancy">
	<id
        name="id"
        type="java.lang.Long"
        column="id"
        length="20"
    >
		<generator class="assigned" />
	</id>
	<property
        name="calId"
        type="java.lang.Long"
        column="cal_id"
        not-null="true"
        length="20"
    />
	<property
        name="relevancyId"
        type="java.lang.Long"
        column="relevancy_id"
        not-null="true"
        length="20"
    />
	<property
        name="relevancyType"
        type="java.lang.Integer"
        column="relevancy_type"
        not-null="true"
        length="4"
    />
	
</class>
</hibernate-mapping>
