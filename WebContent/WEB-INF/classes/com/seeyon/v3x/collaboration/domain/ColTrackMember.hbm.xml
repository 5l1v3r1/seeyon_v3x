<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.collaboration.domain.ColTrackMember" 
    table="col_track_member"
    lazy="true"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="objectId"
        type="long"
        column="object_id"
        length="20"
    />
       <property
        name="affairId"
        type="long"
        column="affair_id"
        length="20"
    />
       <property
        name="memberId"
        type="long"
        column="member_id"
        length="20"
    />
       <property
        name="trackMemberId"
        type="long"
        column="track_member_id"
        length="20"
    />
   
</class>
</hibernate-mapping>
