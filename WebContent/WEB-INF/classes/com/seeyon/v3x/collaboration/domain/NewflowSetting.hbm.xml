<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class
		name="com.seeyon.v3x.collaboration.domain.NewflowSetting"
		table="v3x_newflow_setting"
		lazy="true"
	>
		<id
        name="id"
        type="java.lang.Long"
        column="id"
        length="20"
    	>
			<generator class="assigned"/>
		</id>
		<property
			name="templeteId"
			column="templete_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="nodeId"
			column="node_id"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="newflowTempleteId"
			column="newflow_templete_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="newflowSender"
			column="newflow_sender"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="triggerCondition"
			column="trigger_condition"
			type="text"
			not-null="false"
			length="65535"
		/>
		<property
			name="conditionTitle"
			column="condition_title"
			type="text"
			not-null="false"
	        length="65535"
		/>
		<property
			name="conditionBase"
			column="condition_base"
			type="string"
			not-null="false"
			length="255"
		/>
		<property 
	    	name="flowRelateType"
			type="integer"
			column="flow_relate_type"
			length="11"
    	/>
		<property 
	    	name="isCanViewByMainFlow"
			type="boolean"
			column="can_view_by_main"
			length="4"
    	/>
		<property 
	    	name="isForce"
			type="boolean"
			column="is_force"
			length="4"
    	/>
		<property 
	    	name="isCanViewMainFlow"
			type="boolean"
			column="can_view_main"
			length="4"
    	/>
		<property
			name="createTime"
			column="create_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
	</class>	
</hibernate-mapping>