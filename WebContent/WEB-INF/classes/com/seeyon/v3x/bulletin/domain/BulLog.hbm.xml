<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.seeyon.v3x.bulletin.domain">
	<class
		name="BulLog"
		table="bul_log"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="recordDate"
			column="record_date"
			type="timestamp"
			not-null="true"
			length="10"
		/>
		<property
			name="userId"
			column="user_id"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>
		<property
			name="logType"
			column="log_type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="logLevel"
			column="log_level"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="operType"
			column="oper_type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tableName"
			column="table_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="recordId"
			column="record_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="result"
			column="result"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ext1"
			column="ext1"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ext2"
			column="ext2"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>