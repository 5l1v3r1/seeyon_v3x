<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.seeyon.v3x.taskmanage.domain.TaskRole" lazy="false" table="task_role">
		<id name="id" type="long" column="id" length="20">
			<generator class="assigned" />			
		</id>
		<property name="roleId" type="long" column="role_id" not-null="true" length="20" />
		<property name="roleType" type="integer" column="role_type" not-null="true" length="4" />
		<property name="taskId" type="long" column="task_id" not-null="true" length="20" />
	</class>
</hibernate-mapping>
