<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.seeyon.v3x.mobile.message.domain">
	<class
		name="MobileMessage"
		table="mobile_message"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="long"
			column="id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="objectId"
			column="object_id"
			type="long"
			not-null="false"
			length="20"
		/>
		<property
			name="type"
			column="type"
			type="int"
			not-null="false"
			length="10"
		/>
		<property
			name="uid"
			column="u_id"
			type="long"
			not-null="false"
		/>
		<property
			name="senderId"
			column="sender_Id"
			type="long"
			not-null="false"
			length="20"
		/>
		<property
			name="content"
			column="content"
			type="string"
			not-null="false"
		/>
		<property
			name="departmentId"
			column="department_id"
			type="long"
			not-null="false"
		/>
		<property
			name="messageType"
			column="messagetype"
			type="int"
			not-null="false"
			length="10"
		/>
		<property
			name="time"
			column="time"
			type="java.util.Date"
			not-null="false"
			length="20"
		/>
		<property
			name="senderPhoneNumber"
			column="senderPhoneNumber"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="reciverPhoneNumber"
			column="reciverPhoneNumber"
			type="string"
			not-null="false"
			length="4000"
		/>
		<property
			name="state"
			column="state"
			type="int"
			not-null="false"
			length="10"
		/>
		<property
			name="num"
			column="num"
			type="int"
			not-null="false"
		/>
		<property
			name="featureCode"
			column="feature_code"
			type="string"
			length="4"
		/>
		<property
			name="appSubObjectId"
			column="app_sub_objectId"
			type="long"
			not-null="false"
			length="20"
		/>
		<property
			name="accountId"
			column="account_id"
			type="long"
			not-null="false"
			length="20"
		/>
		<property
			name="smsType"
			column="sms_type"
			type="int"
			not-null="false"
			length="4"
		/>
	</class>	
</hibernate-mapping>