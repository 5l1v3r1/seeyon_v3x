<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.seeyon.v3x.doc.domain.DocMimeType" lazy="true"
		table="doc_mime_types">
		<id name="id" type="long" column="id" length="20">
			<generator class="assigned" />
		</id>
		<property name="name" type="string" column="name"
			not-null="true" length="255" />
		<property name="icon" type="string" column="icon"
			not-null="true" length="255" />
		<property name="formatType" type="long" column="format_type"
			not-null="true" length="20" />
			
								<property name="orderNum" type="integer" column="order_num"
			not-null="true" length="11" />
	
	</class>
</hibernate-mapping>
