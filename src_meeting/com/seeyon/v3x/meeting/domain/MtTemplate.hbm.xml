<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.seeyon.v3x.meeting.domain">
	<class
		name="MtTemplate"
		table="mt_template"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="templateId"
			column="template_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="templateName"
			column="template_name"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="templateType"
			column="template_type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="title"
			column="title"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="meetingType"
			column="meeting_type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="emceeId"
			column="emcee_id"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>
		<property
			name="conferees"
			column="conferees"
			type="clob"
			not-null="true"
		/>
		<property
			name="recorderId"
			column="recorder_id"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>
		<property
			name="projectId"
			column="project_id"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="beginDate"
			column="begin_date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="endDate"
			column="end_date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="remindFlag"
			column="remind_flag"
			type="boolean"
			not-null="true"
			length="1"
		/>
		<property
			name="beforeTime"
			column="before_time"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="address"
			column="address"
			type="string"
			not-null="false"
		/>
		<property
			name="room"
			column="room"
			type="string"
			not-null="false"
		/>
		<property
			name="dataFormat"
			column="data_format"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="content"
			column="content"
			type="clob"
			not-null="false"
		/>
		<property
			name="createUser"
			column="create_user"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>
		<property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="pigeonholePath"
			column="pigeonhole_path"
			type="string"
			not-null="false"
		/>
		<property
			name="updateDate"
			column="update_date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="updateUser"
			column="update_user"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="state"
			column="state"
			type="integer"
			not-null="true"
			length="11"
		/>
		<property
			name="ext1"
			column="ext1"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ext2"
			column="ext2"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ext3"
			column="ext3"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="videoMeetingId"
			column="ext4"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="confKey"
			column="ext5"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="accountId"
			column="account_id"
			type="long"
			not-null="true"
			length="20"
		/>
		<set name="templateUsers" inverse="true" lazy="true"
        cascade="all-delete-orphan">
			<key column="template_id"/>
			<one-to-many class="MtTemplateUser"/>
		</set>


	</class>	
</hibernate-mapping>