<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.collaboration.domain.ColSummary" 
    table="col_summary"
    lazy="false"
    dynamic-update="false"
    dynamic-insert="false"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="archiveId"
        type="long"
        column="archive_id"
        length="20"
    />
    <property
        name="canArchive"
        type="boolean"
        column="can_archive"
        length="4"
    />
    <property
        name="canAutoStopFlow"
        type="boolean"
        column="can_autostopflow"
        length="4"
    />
    <property
        name="canDueReminder"
        type="boolean"
        column="can_due_reminder"
        length="4"
    />
    <property
        name="canForward"
        type="boolean"
        column="can_forward"
        length="4"
    />
    <property
        name="canModify"
        type="boolean"
        column="can_modify"
        length="4"
    />
    
    <property
        name="canEdit"
        type="boolean"
        column="can_edit"
        length="4"
    />
    <property
        name="canEditAttachment"
        type="boolean"
        column="can_edit_attachment"
        length="4"
    />
    <property
        name="canTrack"
        type="boolean"
        column="can_track"
        length="4"
    />
    <property
        name="caseId"
        type="long"
        column="case_id"
        length="20"
    />
    <property
        name="processId"
        type="string"
        column="process_id"
        length="11"
    />
    <property
        name="colType"
        type="integer"
        column="col_type"
        length="11"
    />
    <property
        name="createDate"
        type="timestamp"
        column="create_date"
        length="19"
    />
    <property
        name="deadline"
        type="long"
        column="deadline"
        length="20"
    />
    <property
        name="finishDate"
        type="timestamp"
        column="finish_date"
        length="19"
    />
    <property
        name="importantLevel"
        type="integer"
        column="important_level"
        length="11"
    />
    <property
        name="isAudited"
        type="boolean"
        column="is_audited"
        length="4"
    />
    <property
        name="projectId"
        type="long"
        column="project_id"
        length="20"
    />
    <property
        name="remindInterval"
        type="long"
        column="remind_interval"
        length="20"
    />
    <property
        name="resentTime"
        type="integer"
        column="resent_time"
        length="11"
    />
    <property
        name="startDate"
        type="timestamp"
        column="start_date"
        length="19"
    />
    <property
        name="startMemberId"
        type="long"
        column="start_member_id" 
        not-null="true" 
        length="20"
    />
    <property
        name="state"
        type="integer"
        column="state"
        length="11"
    />
    <property
        name="subject"
        type="string"
        column="subject"
        length="255"
    />
    <property
        name="advanceRemind"
        type="long"
        column="advance_remind"
        length="20"
    />
    <property
        name="identifier"
        type="string"
        column="identifier"
        length="100"
    />
    <property
        name="forwardMember"
        type="string"
        column="forward_member"
        length="255"
    />
    <property
        name="templeteId"
        type="long"
        column="templete_id" 
        length="20"
    />
    <property
        name="workflowRule"
        type="clob"
        column="workflow_rule" 
    />
    
     <property
        name="webServiceCode"
        type="string"
        column="webservice_code" 
        length="50"
    />
    
    <property
        name="bodyType"
        type="string"
        column="body_type"
        length="20"
    />
	
	<property
        name="formAppId"
        type="long"
        column="form_appid"
        length="20"
    />
	
	<property
        name="formId"
        type="long"
        column="formid"
        length="20"
    />
    <property
        name="formRecordId"
        type="long"
        column="form_recordid"
        length="20"
    />
    <property
        name="orgAccountId"
        type="long"
        column="org_account_id"
        length="20"
    />
    <property
        name="orgDepartmentId"
        type="long"
        column="org_department_id"
        length="20"
    />
     <property
        name="runTime"
        type="long"
        column="run_time"
        length="20"
    />
     <property
        name="overTime"
        type="long"
        column="over_time"
        length="20"
    />
     <property
        name="runWorkTime"
        type="long"
        column="run_worktime"
        length="20"
    />
     <property
        name="overWorkTime"
        type="long"
        column="over_worktime"
        length="20"
    />
     <property
        name="source"
        type="string"
        column="source"
        length="255"
    />
    <property
        name="newflowType"
        type="integer"
        column="newflow_type"
        length="11"
    />
      <property
        name="parentformSummaryId"
        type="long"
        column="parentform_summaryid"
        length="20"
    />
        <property
        name="isVouch"
        type="integer"
        column="is_vouch"
        length="1"
    />
    <!-- Associations -->
    <!-- bi-directional many-to-one association to ColBody -->
    <set
        name="bodies"
        lazy="true"
        cascade="all-delete-orphan"
        inverse="true"
    >
        <key>
            <column name="col_id" /> <!-- a foreign key in col_body referencing the primary key of this table. -->
        </key>
        <one-to-many class="com.seeyon.v3x.collaboration.domain.ColBody" />
    </set>
    <!-- bi-directional many-to-one association to ColComment -->
    <set
        name="comments"
        lazy="true"
        inverse="true"    
        cascade="all-delete-orphan"
        order-by="create_date"
    >
        <key>
            <column name="col_id" /> <!-- a foreign key in col_comment referencing the primary key of this table. -->
        </key>
        <one-to-many class="com.seeyon.v3x.collaboration.domain.ColComment" />
    </set>
    <!-- bi-directional many-to-one association to ColOpinion -->
    <set
        name="opinions"
        inverse="true"
        lazy="true"
        cascade="all-delete-orphan"
        order-by="create_date"
    >
        <key>
            <column name="col_id" /> <!-- a foreign key in col_opinion referencing the primary key of this table. -->
        </key>
        <one-to-many class="com.seeyon.v3x.collaboration.domain.ColOpinion" />
    </set>
    <!-- bi-directional many-to-one association to Affair -->
<!--    <set
        name="affairs"
        inverse="true"
        lazy="false"
    >
        <key>
            <column name="col_id" /> --><!-- a foreign key in v3x_affair referencing the primary key of this table. --><!--
        </key>
        <one-to-many class="com.seeyon.v3x.affair.domain.Affair" />
    </set>-->
	<!-- //成发集团项目 程炯 2012-8-29 增加流程密级属性 begin -->
	<property
        name="secretLevel"
        type="integer"
        column="secret_level"
        length="4"
    />
    <!-- //2017-4-12 诚佰公司 附件密级  -->
    <property
        name="attachLevel"
        type="integer"
        column="attach_level"
        length="4"
    />
</class>
</hibernate-mapping>