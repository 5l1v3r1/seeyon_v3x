<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.seeyon.v3x.space.domain">
	<class
		name="SpacePage"
		table="page"
	>
		<id
			name="id"
			type="long"
			column="PAGE_ID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="parentId"
			column="PARENT_ID"
			type="integer"
			not-null="true"
			length="9"
		/>
		<property
			name="path"
			column="PATH"
			type="string"
			not-null="true"
			length="240"
		/>
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="true"
			length="80"
		/>
		<property
			name="version"
			column="VERSION"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="title"
			column="TITLE"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="shortTitle"
			column="SHORT_TITLE"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="isHidden"
			column="IS_HIDDEN"
			type="integer"
			not-null="true"
			length="11"
		/>
		<property
			name="skin"
			column="SKIN"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="defaultLayoutDecorator"
			column="DEFAULT_LAYOUT_DECORATOR"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="defaultPortletDecorator"
			column="DEFAULT_PORTLET_DECORATOR"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="subsite"
			column="SUBSITE"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="userPrincipal"
			column="USER_PRINCIPAL"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="rolePrincipal"
			column="ROLE_PRINCIPAL"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="groupPrincipal"
			column="GROUP_PRINCIPAL"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="mediatype"
			column="MEDIATYPE"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="locale"
			column="LOCALE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="extAttrName"
			column="EXT_ATTR_NAME"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="extAttrValue"
			column="EXT_ATTR_VALUE"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="ownerPrincipal"
			column="OWNER_PRINCIPAL"
			type="string"
			not-null="false"
			length="40"
		/>
	</class>	
</hibernate-mapping>